  <name>Regra_Matematica</name>
  <uuid>e3519b75-7523-454f-a104-5e8905753181</uuid>
  <locale>pt_BR</locale>
  <definition><![CDATA[se 
	 dadosOK é verdadeiro 
então
	Apresentar "****************** LOG **************************************";
	
	atribuir o media matematica  de calcula_Semestre_2 a o nota matematica  de request_nota_Semestre_2;  
	Apresentar "Matematica semestre 2 = " + o nota matematica  de request_nota_Semestre_2;  
	colocar à verdadeiro que calcula_Semestre_2 é status calcula matematica; 

senão 
	Apresentar "****************** LOG **************************************";
	
	colocar à falso que calcula_Semestre_2 é status calcula matematica ; 
	atribuir o matemarica de calcula_Semestre_2 a o matemarica de calcula_Semestre_2 ;
	Apresentar "Comentarios Matematica: " + o matemarica de calcula_Semestre_2 ; ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
