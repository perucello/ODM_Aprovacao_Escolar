  <uuid>125f6bd7-bc58-4ce2-b62f-ecef3f27f931</uuid>
  <locale>pt_BR</locale>
  <definition><![CDATA[se 
	 dadosOK é verdadeiro 
então
	Apresentar "";
	atribuir o media historia de calcula_Semestre_1 a o nota historia de request_nota_Semestre_1 ;  
	Apresentar "Historia semestre 1 = " + o nota historia de request_nota_Semestre_1 ;  
	colocar à verdadeiro que calcula_Semestre_1 é status calcula historia  ; 

senão 
	Apresentar "";
	colocar à falso que calcula_Semestre_1 é status calcula historia  ; 
	atribuir o historia  de calcula_Semestre_1 a o historia  de calcula_Semestre_1 ;
	Apresentar "Comentarios Historia: " + o historia  de calcula_Semestre_1 ; ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
