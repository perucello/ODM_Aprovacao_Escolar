  <name>Regra_Portugues</name>
  <uuid>e5413ba2-e09a-45f9-b1b0-03b9c8cc6573</uuid>
  <locale>pt_BR</locale>
  <definition><![CDATA[se 
	 dadosOK é verdadeiro 
então
	Apresentar "";
	atribuir o media portugues  de calcula_Semestre_2 a o nota portugues de request_nota_Semestre_2;  
	Apresentar "Portugues semestre 2 = " + o nota portugues de request_nota_Semestre_2 ;  
	colocar à verdadeiro que calcula_Semestre_2 é status calcula portugues  ; 

senão 
	Apresentar "";
	colocar à falso que calcula_Semestre_2 é status calcula portugues ; 
	atribuir o portugues  de calcula_Semestre_2 a o portugues de calcula_Semestre_2 ;
	Apresentar "Comentarios Portugues: " + o portugues  de calcula_Semestre_2 ; ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
